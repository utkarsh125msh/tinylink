generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Link {
  id          String   @id @default(uuid())
  code        String   @unique
  target      String
  clicks      Int      @default(0)
  createdAt   DateTime @default(now())
  lastClicked DateTime?
  deleted     Boolean  @default(false)
}
